{
  "name": "wolox-tt",
  "version": "0.1.0",
  "private": true,
  "jest": {
    "collectCoverageFrom": [
      "src/redux/**/*.js",
      "src/util/*.js",
      "!src/redux/index.js",
      "!src/redux/actions/index.js",
      "!src/redux/reducers/index.js",
      "!src/util/index.js",
      "!src/util/global.js"
    ]
  },
  "dependencies": {
    "@material-ui/core": "^4.9.14",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/styles": "^4.9.14",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "axios": "^0.19.2",
    "axios-mock-adapter": "^1.18.1",
    "classnames": "^2.2.6",
    "deepmerge": "^4.2.2",
    "html-react-parser": "^0.13.0",
    "moment": "^2.26.0",
    "node-sass": "^4.14.1",
    "numeral": "^2.0.6",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^6.0.0",
    "react-redux": "^7.2.0",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "react-router-hash-link": "^2.0.0",
    "react-scripts": "^3.4.3",
    "recompose": "^0.30.0",
    "redux": "^4.0.5",
    "redux-logger": "^3.0.6",
    "redux-mock-store": "^1.5.4",
    "redux-thunk": "^2.3.0",
    "webfontloader": "^1.6.28"
  },
  "scripts": {
    "build": "cross-env REACT_APP_ENV=development npm-run-all build-css build-js",
    "build-css": "node-sass --include-path ./src --include-path ./node_modules src/ -o src/",
    "build-js": "react-scripts build",
    "eject": "react-scripts eject",
    "lintj": "./node_modules/.bin/eslint --ext .js src/",
    "lints": "./node_modules/.bin/stylelint \"src/**/*.scss\"",
    "start": "cross-env REACT_APP_ENV=local npm-run-all -p watch-css start-js",
    "start-js": "react-scripts start",
    "test": "cross-env REACT_APP_ENV=unitTest CI=true react-scripts test --colors --transformIgnorePatterns --env=jsdom --coverage",
    "watch-css": "npm run build-css && node-sass --include-path ./src --include-path ./node_modules src/ -o src/ --watch --recursive"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "cross-env": "^7.0.2",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-plugin-jest": "^23.13.1",
    "npm-run-all": "^4.1.5",
    "redux-devtools": "^3.5.0",
    "stylelint": "^13.5.0",
    "stylelint-config-standard": "^20.0.0"
  }
}
